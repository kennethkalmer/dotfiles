## GO
#

# homebrew go root
if [[ -d /usr/local/opt/go ]]; then
  export GOROOT=/usr/local/opt/go/libexec
  export PATH=$GOROOT/bin:$PATH
fi

if [[ -d ~/Code/go ]]; then
  # Add the GOPATH
  export GOPATH=~/Code/go
  export PATH=$GOPATH/bin:$PATH
fi

